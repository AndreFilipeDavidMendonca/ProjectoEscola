			<xs:attribute name="birthDate" type="xs:string"/>
			<xs:attribute name="nif" type="xs:string"/>
			<xs:attribute name="mobileNumber" type="xs:string"/>
			<xs:attribute name="phoneNumber" type="xs:string"/>
			<xs:attribute name="address" type="xs:string"/>
			<xs:attribute name="postalCode" type="xs:string"/>
			<xs:attribute name="locality" type="xs:string"/>
			<xs:attribute name="accessionDate" type="xs:string"/>
			<xs:attribute name="dishonest" type="xs:string"/>
