		</xs:complexType>
	</xs:element>
	<xs:element name="Get_Associated_Service_W">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="Associated_Service_Info" minOccurs="0" maxOccurs="unbounded"/>
