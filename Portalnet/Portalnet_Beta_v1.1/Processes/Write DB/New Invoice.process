<?xml version="1.0" encoding="UTF-8"?>
<pd:ProcessDefinition xmlns:xsl="http://www.w3.org/1999/XSL/Transform" xmlns:pfx="http://www.tibco.com/schemas/Portalnet_Beta_v1.0/Resources/Schemas/Schema.xsd" xmlns:pd="http://xmlns.tibco.com/bw/process/2003" xmlns:pfx2="http://www.tibco.com/pe/DeployedVarsType" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:tib="http://www.tibco.com/bw/xslt/custom-functions" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <xsd:import namespace="http://www.tibco.com/schemas/Portalnet_Beta_v1.0/Resources/Schemas/Schema.xsd" schemaLocation="/Resources/Schemas/Client.xsd"/>
    <pd:name>Processes/Write DB/New Invoice.process</pd:name>
    <pd:startName>Start</pd:startName>
    <pd:startType>
        <xsd:element name="root">
            <xsd:complexType>
                <xsd:sequence>
                    <xsd:element name="ClientNumber" type="xsd:int"/>
                    <xsd:element name="Amount" type="xsd:float"/>
                    <xsd:element name="InvoiceDate" type="xsd:string"/>
                    <xsd:element name="AmountPaid" type="xsd:float"/>
                </xsd:sequence>
            </xsd:complexType>
        </xsd:element>
    </pd:startType>
    <pd:startX>27</pd:startX>
    <pd:startY>34</pd:startY>
    <pd:returnBindings>
        <root>
            <message>
                <xsl:value-of select="$Var/root/message"/>
            </message>
            <xsl:if test="$Var/root/error">
                <error>
                    <xsl:value-of select="$Var/root/error"/>
                </error>
            </xsl:if>
        </root>
    </pd:returnBindings>
    <pd:endName>End</pd:endName>
    <pd:endType>
        <xsd:element name="root">
            <xsd:complexType>
                <xsd:sequence>
                    <xsd:element name="message" type="xsd:string"/>
                    <xsd:element name="error" type="xsd:string" minOccurs="0"/>
                </xsd:sequence>
            </xsd:complexType>
        </xsd:element>
    </pd:endType>
    <pd:endX>478</pd:endX>
    <pd:endY>35</pd:endY>
    <pd:errorSchemas/>
    <pd:processVariables>
        <Var>
            <xsd:element name="root">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="message" type="xsd:string"/>
                        <xsd:element name="error" type="xsd:string" minOccurs="0"/>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
        </Var>
    </pd:processVariables>
    <pd:targetNamespace>http://xmlns.example.com/1562688164648</pd:targetNamespace>
    <pd:activity name="Insert Invoice">
        <pd:type>com.tibco.plugin.jdbc.JDBCUpdateActivity</pd:type>
        <pd:resourceType>ae.activities.JDBCUpdateActivity</pd:resourceType>
        <pd:x>177</pd:x>
        <pd:y>35</pd:y>
        <config>
            <timeout>10</timeout>
            <commit>false</commit>
            <emptyStrAsNil>false</emptyStrAsNil>
            <statement>INSERT INTO invoices (clientnumber, amount, invoicedate, amountpaid)
VALUES ( ?, ?, ?, ?)</statement>
            <jdbcSharedConfig>/Resources/JDBC/JDBC Connection.sharedjdbc</jdbcSharedConfig>
            <Prepared_Param_DataType>
                <parameter>
                    <parameterName>ClientNumber</parameterName>
                    <dataType>INTEGER</dataType>
                </parameter>
                <parameter>
                    <parameterName>Amount</parameterName>
                    <dataType>FLOAT</dataType>
                </parameter>
                <parameter>
                    <parameterName>InvoiceDate</parameterName>
                    <dataType>VARCHAR</dataType>
                </parameter>
                <parameter>
                    <parameterName>AmountPaid</parameterName>
                    <dataType>FLOAT</dataType>
                </parameter>
            </Prepared_Param_DataType>
        </config>
        <pd:inputBindings>
            <jdbcUpdateActivityInput>
                <ClientNumber>
                    <xsl:value-of select="$Start/root/ClientNumber"/>
                </ClientNumber>
                <Amount>
                    <xsl:value-of select="$Start/root/Amount"/>
                </Amount>
                <InvoiceDate>
                    <xsl:value-of select="$Start/root/InvoiceDate"/>
                </InvoiceDate>
                <AmountPaid>
                    <xsl:value-of select="$Start/root/AmountPaid"/>
                </AmountPaid>
            </jdbcUpdateActivityInput>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Assign Success">
        <pd:type>com.tibco.pe.core.AssignActivity</pd:type>
        <pd:resourceType>ae.activities.assignActivity</pd:resourceType>
        <pd:x>337</pd:x>
        <pd:y>34</pd:y>
        <config>
            <variableName>Var</variableName>
        </config>
        <pd:inputBindings>
            <root>
                <message>
                    <xsl:value-of select="$_globalVariables/pfx2:GlobalVariables/VarAplication/MessageType/Success"/>
                </message>
            </root>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Assign Error">
        <pd:type>com.tibco.pe.core.AssignActivity</pd:type>
        <pd:resourceType>ae.activities.assignActivity</pd:resourceType>
        <pd:x>620</pd:x>
        <pd:y>34</pd:y>
        <config>
            <variableName>Var</variableName>
        </config>
        <pd:inputBindings>
            <root>
                <message>
                    <xsl:value-of select="$_globalVariables/pfx2:GlobalVariables/VarAplication/MessageType/Error"/>
                </message>
                <error>
                    <xsl:value-of select="&quot;New Invoice-Invalid Input&quot;"/>
                </error>
            </root>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Catch">
        <pd:type>com.tibco.pe.core.CatchActivity</pd:type>
        <pd:resourceType>ae.activities.catch</pd:resourceType>
        <pd:x>758</pd:x>
        <pd:y>34</pd:y>
        <pd:handler>true</pd:handler>
        <config>
            <catchAll>true</catchAll>
        </config>
        <pd:inputBindings/>
    </pd:activity>
    <pd:transition>
        <pd:from>Insert Invoice</pd:from>
        <pd:to>Assign Success</pd:to>
        <pd:xpathDescription>updated</pd:xpathDescription>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>xpath</pd:conditionType>
        <pd:xpath>$Insert-Invoice/jdbcUpdateActivityOutput/noOfUpdates &gt; 0</pd:xpath>
    </pd:transition>
    <pd:transition>
        <pd:from>Assign Success</pd:from>
        <pd:to>End</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Assign Error</pd:from>
        <pd:to>End</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Catch</pd:from>
        <pd:to>Assign Error</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Start</pd:from>
        <pd:to>Insert Invoice</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
</pd:ProcessDefinition>