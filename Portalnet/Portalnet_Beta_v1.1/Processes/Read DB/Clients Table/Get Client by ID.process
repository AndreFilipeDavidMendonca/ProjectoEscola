<?xml version="1.0" encoding="UTF-8"?>
<pd:ProcessDefinition xmlns:xsl="http://www.w3.org/1999/XSL/Transform" xmlns:pd="http://xmlns.tibco.com/bw/process/2003" xmlns:ns="http://www.tibco.com/pe/DeployedVarsType" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:tib="http://www.tibco.com/bw/xslt/custom-functions">
    <pd:name>Processes/Read DB/Clients Table/Get Client by ID.process</pd:name>
    <pd:startName>Start</pd:startName>
    <pd:startType>
        <xsd:element name="root">
            <xsd:complexType>
                <xsd:sequence>
                    <xsd:element name="ClientNumber" type="xsd:int"/>
                </xsd:sequence>
            </xsd:complexType>
        </xsd:element>
    </pd:startType>
    <pd:startX>27</pd:startX>
    <pd:startY>34</pd:startY>
    <pd:returnBindings>
        <root>
            <message>
                <xsl:value-of select="$Var/root/message"/>
            </message>
            <xsl:if test="$Var/root/error">
                <error>
                    <xsl:value-of select="$Var/root/error"/>
                </error>
            </xsl:if>
            <xsl:for-each select="$Query-Client/resultSet/Record">
                <ClientInfo>
                    <ClientName>
                        <xsl:value-of select="clientname"/>
                    </ClientName>
                    <Email>
                        <xsl:value-of select="email"/>
                    </Email>
                    <Gender>
                        <xsl:value-of select="gender"/>
                    </Gender>
                    <BirthDate>
                        <xsl:value-of select="birthdate"/>
                    </BirthDate>
                    <NIF>
                        <xsl:value-of select="nif"/>
                    </NIF>
                    <MobileNumber>
                        <xsl:value-of select="mobilenumber"/>
                    </MobileNumber>
                    <PhoneNumber>
                        <xsl:value-of select="phonenumber"/>
                    </PhoneNumber>
                    <Address>
                        <xsl:value-of select="address"/>
                    </Address>
                    <PostalCode>
                        <xsl:value-of select="postalcode"/>
                    </PostalCode>
                    <Locality>
                        <xsl:value-of select="locality"/>
                    </Locality>
                    <AccessionDate>
                        <xsl:value-of select="accessiondate"/>
                    </AccessionDate>
                    <Dishonest>
                        <xsl:value-of select="dishonest"/>
                    </Dishonest>
                    <Active>
                        <xsl:value-of select="active"/>
                    </Active>
                    <WaitConfirmation>
                        <xsl:value-of select="waitconfirmation"/>
                    </WaitConfirmation>
                </ClientInfo>
            </xsl:for-each>
        </root>
    </pd:returnBindings>
    <pd:endName>End</pd:endName>
    <pd:endType>
        <xsd:element name="root">
            <xsd:complexType>
                <xsd:sequence>
                    <xsd:element name="message" type="xsd:string"/>
                    <xsd:element name="error" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="ClientInfo" minOccurs="0" maxOccurs="unbounded">
                        <xsd:complexType>
                            <xsd:sequence>
                                <xsd:element name="ClientName" type="xsd:string"/>
                                <xsd:element name="Email" type="xsd:string"/>
                                <xsd:element name="Gender" type="xsd:string"/>
                                <xsd:element name="BirthDate" type="xsd:string"/>
                                <xsd:element name="NIF" type="xsd:int"/>
                                <xsd:element name="MobileNumber" type="xsd:int" minOccurs="0"/>
                                <xsd:element name="PhoneNumber" type="xsd:int" minOccurs="0"/>
                                <xsd:element name="Address" type="xsd:string"/>
                                <xsd:element name="PostalCode" type="xsd:string"/>
                                <xsd:element name="Locality" type="xsd:string"/>
                                <xsd:element name="AccessionDate" type="xsd:string"/>
                                <xsd:element name="Dishonest" type="xsd:boolean"/>
                                <xsd:element name="Active" type="xsd:boolean"/>
                                <xsd:element name="WaitConfirmation" type="xsd:boolean"/>
                            </xsd:sequence>
                        </xsd:complexType>
                    </xsd:element>
                </xsd:sequence>
            </xsd:complexType>
        </xsd:element>
    </pd:endType>
    <pd:endX>500</pd:endX>
    <pd:endY>150</pd:endY>
    <pd:errorSchemas/>
    <pd:processVariables>
        <Var>
            <xsd:element name="root">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="message" type="xsd:string"/>
                        <xsd:element name="error" type="xsd:string" minOccurs="0"/>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
        </Var>
    </pd:processVariables>
    <pd:targetNamespace>http://xmlns.example.com/1562769332636</pd:targetNamespace>
    <pd:activity name="Counter">
        <pd:type>com.tibco.plugin.jdbc.JDBCQueryActivity</pd:type>
        <pd:resourceType>ae.activities.JDBCQueryActivity</pd:resourceType>
        <pd:x>165</pd:x>
        <pd:y>34</pd:y>
        <config>
            <timeout>10</timeout>
            <commit>false</commit>
            <maxRows>100</maxRows>
            <emptyStrAsNil>false</emptyStrAsNil>
            <jdbcSharedConfig>/Resources/JDBC/JDBC Connection.sharedjdbc</jdbcSharedConfig>
            <statement>SELECT COUNT(*) 
FROM clients 
WHERE clientnumber = ?</statement>
            <Prepared_Param_DataType>
                <parameter>
                    <parameterName>ClientNumber</parameterName>
                    <dataType>INTEGER</dataType>
                </parameter>
            </Prepared_Param_DataType>
            <oraObjects/>
            <oraTables/>
            <QueryOutputCachedSchemaColumns>COUNT</QueryOutputCachedSchemaColumns>
            <QueryOutputCachedSchemaDataTypes>-5</QueryOutputCachedSchemaDataTypes>
            <QueryOutputCachedSchemaDataTypesName>BIGINT</QueryOutputCachedSchemaDataTypesName>
            <QueryOutputCachedSchemaStatus>RequiredElement</QueryOutputCachedSchemaStatus>
        </config>
        <pd:inputBindings>
            <jdbcQueryActivityInput>
                <ClientNumber>
                    <xsl:value-of select="$Start/root/ClientNumber"/>
                </ClientNumber>
            </jdbcQueryActivityInput>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Query Client">
        <pd:type>com.tibco.plugin.jdbc.JDBCQueryActivity</pd:type>
        <pd:resourceType>ae.activities.JDBCQueryActivity</pd:resourceType>
        <pd:x>333</pd:x>
        <pd:y>34</pd:y>
        <config>
            <timeout>10</timeout>
            <commit>false</commit>
            <maxRows>100</maxRows>
            <emptyStrAsNil>false</emptyStrAsNil>
            <jdbcSharedConfig>/Resources/JDBC/JDBC Connection.sharedjdbc</jdbcSharedConfig>
            <statement>SELECT clientname, email, gender, birthdate, nif, mobilenumber, phonenumber, address, postalcode, locality, accessiondate, dishonest, active, waitconfirmation
FROM clients 
WHERE clientnumber = ?</statement>
            <Prepared_Param_DataType>
                <parameter>
                    <parameterName>ClientNumber</parameterName>
                    <dataType>INTEGER</dataType>
                </parameter>
            </Prepared_Param_DataType>
            <oraObjects/>
            <oraTables/>
            <QueryOutputCachedSchemaColumns>clientname</QueryOutputCachedSchemaColumns>
            <QueryOutputCachedSchemaDataTypes>12</QueryOutputCachedSchemaDataTypes>
            <QueryOutputCachedSchemaDataTypesName>VARCHAR</QueryOutputCachedSchemaDataTypesName>
            <QueryOutputCachedSchemaStatus>RequiredElement</QueryOutputCachedSchemaStatus>
            <QueryOutputCachedSchemaColumns>email</QueryOutputCachedSchemaColumns>
            <QueryOutputCachedSchemaDataTypes>12</QueryOutputCachedSchemaDataTypes>
            <QueryOutputCachedSchemaDataTypesName>VARCHAR</QueryOutputCachedSchemaDataTypesName>
            <QueryOutputCachedSchemaStatus>RequiredElement</QueryOutputCachedSchemaStatus>
            <QueryOutputCachedSchemaColumns>gender</QueryOutputCachedSchemaColumns>
            <QueryOutputCachedSchemaDataTypes>12</QueryOutputCachedSchemaDataTypes>
            <QueryOutputCachedSchemaDataTypesName>VARCHAR</QueryOutputCachedSchemaDataTypesName>
            <QueryOutputCachedSchemaStatus>RequiredElement</QueryOutputCachedSchemaStatus>
            <QueryOutputCachedSchemaColumns>birthdate</QueryOutputCachedSchemaColumns>
            <QueryOutputCachedSchemaDataTypes>12</QueryOutputCachedSchemaDataTypes>
            <QueryOutputCachedSchemaDataTypesName>VARCHAR</QueryOutputCachedSchemaDataTypesName>
            <QueryOutputCachedSchemaStatus>RequiredElement</QueryOutputCachedSchemaStatus>
            <QueryOutputCachedSchemaColumns>nif</QueryOutputCachedSchemaColumns>
            <QueryOutputCachedSchemaDataTypes>4</QueryOutputCachedSchemaDataTypes>
            <QueryOutputCachedSchemaDataTypesName>INT</QueryOutputCachedSchemaDataTypesName>
            <QueryOutputCachedSchemaStatus>RequiredElement</QueryOutputCachedSchemaStatus>
            <QueryOutputCachedSchemaColumns>mobilenumber</QueryOutputCachedSchemaColumns>
            <QueryOutputCachedSchemaDataTypes>4</QueryOutputCachedSchemaDataTypes>
            <QueryOutputCachedSchemaDataTypesName>INT</QueryOutputCachedSchemaDataTypesName>
            <QueryOutputCachedSchemaStatus>RequiredElement</QueryOutputCachedSchemaStatus>
            <QueryOutputCachedSchemaColumns>phonenumber</QueryOutputCachedSchemaColumns>
            <QueryOutputCachedSchemaDataTypes>4</QueryOutputCachedSchemaDataTypes>
            <QueryOutputCachedSchemaDataTypesName>INT</QueryOutputCachedSchemaDataTypesName>
            <QueryOutputCachedSchemaStatus>RequiredElement</QueryOutputCachedSchemaStatus>
            <QueryOutputCachedSchemaColumns>address</QueryOutputCachedSchemaColumns>
            <QueryOutputCachedSchemaDataTypes>12</QueryOutputCachedSchemaDataTypes>
            <QueryOutputCachedSchemaDataTypesName>VARCHAR</QueryOutputCachedSchemaDataTypesName>
            <QueryOutputCachedSchemaStatus>RequiredElement</QueryOutputCachedSchemaStatus>
            <QueryOutputCachedSchemaColumns>postalcode</QueryOutputCachedSchemaColumns>
            <QueryOutputCachedSchemaDataTypes>12</QueryOutputCachedSchemaDataTypes>
            <QueryOutputCachedSchemaDataTypesName>VARCHAR</QueryOutputCachedSchemaDataTypesName>
            <QueryOutputCachedSchemaStatus>RequiredElement</QueryOutputCachedSchemaStatus>
            <QueryOutputCachedSchemaColumns>locality</QueryOutputCachedSchemaColumns>
            <QueryOutputCachedSchemaDataTypes>12</QueryOutputCachedSchemaDataTypes>
            <QueryOutputCachedSchemaDataTypesName>VARCHAR</QueryOutputCachedSchemaDataTypesName>
            <QueryOutputCachedSchemaStatus>RequiredElement</QueryOutputCachedSchemaStatus>
            <QueryOutputCachedSchemaColumns>accessiondate</QueryOutputCachedSchemaColumns>
            <QueryOutputCachedSchemaDataTypes>12</QueryOutputCachedSchemaDataTypes>
            <QueryOutputCachedSchemaDataTypesName>VARCHAR</QueryOutputCachedSchemaDataTypesName>
            <QueryOutputCachedSchemaStatus>RequiredElement</QueryOutputCachedSchemaStatus>
            <QueryOutputCachedSchemaColumns>dishonest</QueryOutputCachedSchemaColumns>
            <QueryOutputCachedSchemaDataTypes>-7</QueryOutputCachedSchemaDataTypes>
            <QueryOutputCachedSchemaDataTypesName>TINYINT</QueryOutputCachedSchemaDataTypesName>
            <QueryOutputCachedSchemaStatus>RequiredElement</QueryOutputCachedSchemaStatus>
            <QueryOutputCachedSchemaColumns>active</QueryOutputCachedSchemaColumns>
            <QueryOutputCachedSchemaDataTypes>-7</QueryOutputCachedSchemaDataTypes>
            <QueryOutputCachedSchemaDataTypesName>TINYINT</QueryOutputCachedSchemaDataTypesName>
            <QueryOutputCachedSchemaStatus>RequiredElement</QueryOutputCachedSchemaStatus>
            <QueryOutputCachedSchemaColumns>waitconfirmation</QueryOutputCachedSchemaColumns>
            <QueryOutputCachedSchemaDataTypes>-7</QueryOutputCachedSchemaDataTypes>
            <QueryOutputCachedSchemaDataTypesName>TINYINT</QueryOutputCachedSchemaDataTypesName>
            <QueryOutputCachedSchemaStatus>RequiredElement</QueryOutputCachedSchemaStatus>
        </config>
        <pd:inputBindings>
            <jdbcQueryActivityInput>
                <ClientNumber>
                    <xsl:value-of select="$Start/root/ClientNumber"/>
                </ClientNumber>
            </jdbcQueryActivityInput>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Assign Success">
        <pd:type>com.tibco.pe.core.AssignActivity</pd:type>
        <pd:resourceType>ae.activities.assignActivity</pd:resourceType>
        <pd:x>497</pd:x>
        <pd:y>34</pd:y>
        <config>
            <variableName>Var</variableName>
        </config>
        <pd:inputBindings>
            <root>
                <message>
                    <xsl:value-of select="$_globalVariables/ns:GlobalVariables/VarAplication/MessageType/Exist"/>
                </message>
                <error>
                    <xsl:value-of select="&quot;Get Client by ID-Client Already Exist&quot;"/>
                </error>
            </root>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Assign Error 1">
        <pd:type>com.tibco.pe.core.AssignActivity</pd:type>
        <pd:resourceType>ae.activities.assignActivity</pd:resourceType>
        <pd:x>167</pd:x>
        <pd:y>148</pd:y>
        <config>
            <variableName>Var</variableName>
        </config>
        <pd:inputBindings>
            <xsl:choose>
                <xsl:when test="$Counter/resultSet/Record/COUNT = 0">
                    <root>
                        <message>
                            <xsl:value-of select="$_globalVariables/ns:GlobalVariables/VarAplication/MessageType/DontExist"/>
                        </message>
                        <error>
                            <xsl:value-of select="&quot;Get Client by ID-Client Don't Exist&quot;"/>
                        </error>
                    </root>
                </xsl:when>
                <xsl:otherwise>
                    <root>
                        <message>
                            <xsl:value-of select="$_globalVariables/ns:GlobalVariables/VarAplication/MessageType/Error"/>
                        </message>
                        <error>
                            <xsl:value-of select="&quot;Get Client by ID-Found More Than One Client Number&quot;"/>
                        </error>
                    </root>
                </xsl:otherwise>
            </xsl:choose>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Assign Error 2">
        <pd:type>com.tibco.pe.core.AssignActivity</pd:type>
        <pd:resourceType>ae.activities.assignActivity</pd:resourceType>
        <pd:x>672</pd:x>
        <pd:y>148</pd:y>
        <config>
            <variableName>Var</variableName>
        </config>
        <pd:inputBindings>
            <root>
                <message>
                    <xsl:value-of select="$_globalVariables/ns:GlobalVariables/VarAplication/MessageType/Error"/>
                </message>
                <error>
                    <xsl:value-of select="&quot;Get Client by ID-Invalid Input&quot;"/>
                </error>
            </root>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Catch">
        <pd:type>com.tibco.pe.core.CatchActivity</pd:type>
        <pd:resourceType>ae.activities.catch</pd:resourceType>
        <pd:x>669</pd:x>
        <pd:y>34</pd:y>
        <pd:handler>true</pd:handler>
        <config>
            <catchAll>true</catchAll>
        </config>
        <pd:inputBindings/>
    </pd:activity>
    <pd:transition>
        <pd:from>Start</pd:from>
        <pd:to>Counter</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Counter</pd:from>
        <pd:to>Query Client</pd:to>
        <pd:xpathDescription>exist</pd:xpathDescription>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>xpath</pd:conditionType>
        <pd:xpath>$Counter/resultSet/Record/COUNT = 1</pd:xpath>
    </pd:transition>
    <pd:transition>
        <pd:from>Assign Success</pd:from>
        <pd:to>End</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Query Client</pd:from>
        <pd:to>Assign Success</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Counter</pd:from>
        <pd:to>Assign Error 1</pd:to>
        <pd:xpathDescription>error</pd:xpathDescription>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>xpath</pd:conditionType>
        <pd:xpath>not( $Counter/resultSet/Record/COUNT = 1 )</pd:xpath>
    </pd:transition>
    <pd:transition>
        <pd:from>Assign Error 1</pd:from>
        <pd:to>End</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Assign Error 2</pd:from>
        <pd:to>End</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Catch</pd:from>
        <pd:to>Assign Error 2</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
</pd:ProcessDefinition>