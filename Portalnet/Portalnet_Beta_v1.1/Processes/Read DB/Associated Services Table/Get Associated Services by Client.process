<?xml version="1.0" encoding="UTF-8"?>
<pd:ProcessDefinition xmlns:xsl="http://www.w3.org/1999/XSL/Transform" xmlns:pd="http://xmlns.tibco.com/bw/process/2003" xmlns:ns="http://www.tibco.com/pe/DeployedVarsType" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:tib="http://www.tibco.com/bw/xslt/custom-functions">
    <pd:name>Processes/Read DB/Associated Services Table/Get Associated Services by Client.process</pd:name>
    <pd:startName>Start</pd:startName>
    <pd:startType>
        <xsd:element name="root">
            <xsd:complexType>
                <xsd:sequence>
                    <xsd:element name="ClientNumber" type="xsd:int"/>
                </xsd:sequence>
            </xsd:complexType>
        </xsd:element>
    </pd:startType>
    <pd:startX>27</pd:startX>
    <pd:startY>35</pd:startY>
    <pd:returnBindings>
        <root>
            <message>
                <xsl:value-of select="$Var/root/message"/>
            </message>
            <xsl:if test="$Var/root/error">
                <error>
                    <xsl:value-of select="$Var/root/error"/>
                </error>
            </xsl:if>
            <xsl:for-each select="$Query-Associated-Services/resultSet/Record">
                <AssociatedServices>
                    <AssociatedServiceID>
                        <xsl:value-of select="associatedserviceid"/>
                    </AssociatedServiceID>
                    <ServiceID>
                        <xsl:value-of select="serviceid"/>
                    </ServiceID>
                    <WorkerNumber>
                        <xsl:value-of select="workernumber"/>
                    </WorkerNumber>
                    <ModificationDate>
                        <xsl:value-of select="modificationdate"/>
                    </ModificationDate>
                    <Address>
                        <xsl:value-of select="address"/>
                    </Address>
                    <PostalCode>
                        <xsl:value-of select="postalcode"/>
                    </PostalCode>
                    <Locality>
                        <xsl:value-of select="locality"/>
                    </Locality>
                </AssociatedServices>
            </xsl:for-each>
        </root>
    </pd:returnBindings>
    <pd:endName>End</pd:endName>
    <pd:endType>
        <xsd:element name="root">
            <xsd:complexType>
                <xsd:sequence>
                    <xsd:element name="message" type="xsd:string"/>
                    <xsd:element name="error" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="AssociatedServices" minOccurs="0" maxOccurs="unbounded">
                        <xsd:complexType>
                            <xsd:sequence>
                                <xsd:element name="AssociatedServiceID" type="xsd:int"/>
                                <xsd:element name="ServiceID" type="xsd:int"/>
                                <xsd:element name="WorkerNumber" type="xsd:int"/>
                                <xsd:element name="ModificationDate" type="xsd:string"/>
                                <xsd:element name="Address" type="xsd:string"/>
                                <xsd:element name="PostalCode" type="xsd:string"/>
                                <xsd:element name="Locality" type="xsd:string"/>
                            </xsd:sequence>
                        </xsd:complexType>
                    </xsd:element>
                </xsd:sequence>
            </xsd:complexType>
        </xsd:element>
    </pd:endType>
    <pd:endX>500</pd:endX>
    <pd:endY>148</pd:endY>
    <pd:errorSchemas/>
    <pd:processVariables>
        <Var>
            <xsd:element name="root">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="message" type="xsd:string"/>
                        <xsd:element name="error" type="xsd:string" minOccurs="0"/>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
        </Var>
    </pd:processVariables>
    <pd:targetNamespace>http://xmlns.example.com/1562769332636</pd:targetNamespace>
    <pd:activity name="Counter">
        <pd:type>com.tibco.plugin.jdbc.JDBCQueryActivity</pd:type>
        <pd:resourceType>ae.activities.JDBCQueryActivity</pd:resourceType>
        <pd:x>165</pd:x>
        <pd:y>34</pd:y>
        <config>
            <timeout>10</timeout>
            <commit>false</commit>
            <maxRows>100</maxRows>
            <emptyStrAsNil>false</emptyStrAsNil>
            <jdbcSharedConfig>/Resources/JDBC/JDBC Connection.sharedjdbc</jdbcSharedConfig>
            <statement>SELECT COUNT(*) 
FROM associatedservices 
WHERE clientnumber = ?</statement>
            <Prepared_Param_DataType>
                <parameter>
                    <parameterName>ClientNumber</parameterName>
                    <dataType>INTEGER</dataType>
                </parameter>
            </Prepared_Param_DataType>
            <oraObjects/>
            <oraTables/>
            <QueryOutputCachedSchemaColumns>COUNT</QueryOutputCachedSchemaColumns>
            <QueryOutputCachedSchemaDataTypes>-5</QueryOutputCachedSchemaDataTypes>
            <QueryOutputCachedSchemaDataTypesName>BIGINT</QueryOutputCachedSchemaDataTypesName>
            <QueryOutputCachedSchemaStatus>RequiredElement</QueryOutputCachedSchemaStatus>
        </config>
        <pd:inputBindings>
            <jdbcQueryActivityInput>
                <ClientNumber>
                    <xsl:value-of select="$Start/root/ClientNumber"/>
                </ClientNumber>
            </jdbcQueryActivityInput>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Query Associated Services">
