        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>xpath</pd:conditionType>
        <pd:xpath>$Get-Service-by-ID/root/message = $_globalVariables/pfx3:GlobalVariables/VarAplication/MessageType/Exist</pd:xpath>
    </pd:transition>
