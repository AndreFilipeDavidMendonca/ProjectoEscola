        <pd:to>Get Worker by ID</pd:to>
        <pd:xpathDescription>validation</pd:xpathDescription>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>xpath</pd:conditionType>
        <pd:xpath>$Start/pfx11:Change_Service_In_msg/header/pfx9:Standard_In_MSG/@id &gt; 0 and $Start/pfx11:Change_Service_In_msg/header/pfx9:Standard_In_MSG/@id &lt; 10000000000
and
$Start/pfx11:Change_Service_In_msg/header/pfx9:Standard_In_MSG/@role = $_globalVariables/pfx3:GlobalVariables/VarAplication/RoleType/Admin
and 
$Start/pfx11:Change_Service_In_msg/body/pfx10:Modify_Service_A/@serviceID &gt; 0 and $Start/pfx11:Change_Service_In_msg/body/pfx10:Modify_Service_A/@serviceID &lt; 100000</pd:xpath>
    </pd:transition>
    <pd:transition>
        <pd:from>Get Worker by ID</pd:from>
        <pd:to>Get Service by ID</pd:to>
        <pd:xpathDescription>valid_worker</pd:xpathDescription>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>xpath</pd:conditionType>
        <pd:xpath>$Get-Worker-by-ID/root/message = $_globalVariables/pfx3:GlobalVariables/VarAplication/MessageType/Exist
and
$Start/pfx11:Change_Service_In_msg/header/pfx9:Standard_In_MSG/@role = $Get-Worker-by-ID/root/WorkerInfo/Role</pd:xpath>
    </pd:transition>
    <pd:transition>
        <pd:from>Get Service by ID</pd:from>
        <pd:to>Modify Service</pd:to>
        <pd:xpathDescription>valid_service</pd:xpathDescription>
