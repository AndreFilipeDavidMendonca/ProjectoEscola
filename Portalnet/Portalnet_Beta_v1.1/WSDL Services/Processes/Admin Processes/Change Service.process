                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="message" type="xsd:string"/>
                        <xsd:element name="error" type="xsd:string" minOccurs="0"/>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
        </Var>
    </pd:processVariables>
    <pd:targetNamespace>http://xmlns.example.com/1563208666862</pd:targetNamespace>
    <pd:activity name="Get Worker by ID">
        <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
        <pd:resourceType>ae.process.subprocess</pd:resourceType>
        <pd:x>172</pd:x>
        <pd:y>34</pd:y>
        <config>
            <processName>/Processes/Read DB/Workers Table/Get Worker by ID.process</processName>
        </config>
        <pd:inputBindings>
            <root>
