    <wsdl:import namespace="http://xmlns.example.com/1563529202131" location="/WSDL Services/Aplication/Admin WSDL.wsdl"/>
    <pd:name>WSDL Services/Processes/Admin Processes/Create New Service.process</pd:name>
    <pd:startName>Start</pd:startName>
    <pd:startType wsMsgRef="pfx8:New_Service_In_msg"/>
    <pd:startX>27</pd:startX>
    <pd:startY>35</pd:startY>
    <pd:returnBindings>
        <pfx8:New_Service_Out_msg>
            <header>
                <pfx7:Standard_Out_MSG>
                    <xsl:attribute name="message">
                        <xsl:value-of select="$Var/root/message"/>
                    </xsl:attribute>
                    <xsl:if test="$Var/root/error">
                        <xsl:attribute name="error">
                            <xsl:value-of select="$Var/root/error"/>
                        </xsl:attribute>
                    </xsl:if>
                </pfx7:Standard_Out_MSG>
            </header>
        </pfx8:New_Service_Out_msg>
    </pd:returnBindings>
    <pd:endName>End</pd:endName>
    <pd:endType wsMsgRef="pfx8:New_Service_Out_msg"/>
    <pd:endX>737</pd:endX>
    <pd:endY>34</pd:endY>
    <pd:errorSchemas/>
    <pd:processVariables>
        <Var>
            <xsd:element name="root">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="message" type="xsd:string"/>
                        <xsd:element name="error" type="xsd:string" minOccurs="0"/>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
        </Var>
    </pd:processVariables>
    <pd:targetNamespace>http://xmlns.example.com/1563208666862</pd:targetNamespace>
    <pd:activity name="New Service">
        <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
        <pd:resourceType>ae.process.subprocess</pd:resourceType>
        <pd:x>474</pd:x>
        <pd:y>34</pd:y>
        <config>
            <processName>/Processes/Write DB/New Service.process</processName>
        </config>
        <pd:inputBindings>
            <root>
                <Name>
                    <xsl:value-of select="$Start/pfx8:New_Service_In_msg/body/ns1:New_Service_A/@serviceName"/>
                </Name>
                <Internet>
                    <xsl:value-of select="$Start/pfx8:New_Service_In_msg/body/ns1:New_Service_A/@internet"/>
                </Internet>
                <TV>
                    <xsl:value-of select="$Start/pfx8:New_Service_In_msg/body/ns1:New_Service_A/@tv"/>
                </TV>
                <MobilePhone>
                    <xsl:value-of select="$Start/pfx8:New_Service_In_msg/body/ns1:New_Service_A/@mobilePhone"/>
                </MobilePhone>
                <Phone>
                    <xsl:value-of select="$Start/pfx8:New_Service_In_msg/body/ns1:New_Service_A/@phone"/>
                </Phone>
                <Loyalty>
                    <xsl:value-of select="$Start/pfx8:New_Service_In_msg/body/ns1:New_Service_A/@loyalty"/>
                </Loyalty>
                <Price>
                    <xsl:value-of select="$Start/pfx8:New_Service_In_msg/body/ns1:New_Service_A/@price"/>
                </Price>
                <CreationDate>
                    <xsl:value-of select="$Start/pfx8:New_Service_In_msg/body/ns1:New_Service_A/@creationDate"/>
                </CreationDate>
                <xsl:choose>
                    <xsl:when test="$Start/pfx8:New_Service_In_msg/body/ns1:New_Service_A/@active = &quot;true&quot;">
                        <Active>
                            <xsl:value-of select="1"/>
                        </Active>
                    </xsl:when>
                    <xsl:otherwise>
                        <Active>
                            <xsl:value-of select="0"/>
                        </Active>
                    </xsl:otherwise>
                </xsl:choose>
                <ImageURL>
                    <xsl:value-of select="$Start/pfx8:New_Service_In_msg/body/ns1:New_Service_A/@ImageURL"/>
                </ImageURL>
            </root>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Get Worker by ID">
        <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
        <pd:resourceType>ae.process.subprocess</pd:resourceType>
        <pd:x>176</pd:x>
        <pd:y>34</pd:y>
        <config>
            <processName>/Processes/Read DB/Workers Table/Get Worker by ID.process</processName>
        </config>
        <pd:inputBindings>
            <root>
                <WorkerNumber>
                    <xsl:value-of select="$Start/pfx8:New_Service_In_msg/header/pfx7:Standard_In_MSG/@id"/>
                </WorkerNumber>
            </root>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Assign 1">
        <pd:type>com.tibco.pe.core.AssignActivity</pd:type>
        <pd:resourceType>ae.activities.assignActivity</pd:resourceType>
        <pd:x>610</pd:x>
        <pd:y>34</pd:y>
        <config>
            <variableName>Var</variableName>
        </config>
        <pd:inputBindings>
            <root>
                <message>
                    <xsl:value-of select="$New-Service/root/message"/>
                </message>
                <xsl:if test="$New-Service/root/error">
                    <error>
                        <xsl:value-of select="$New-Service/root/error"/>
                    </error>
                </xsl:if>
            </root>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Assign 2">
        <pd:type>com.tibco.pe.core.AssignActivity</pd:type>
        <pd:resourceType>ae.activities.assignActivity</pd:resourceType>
        <pd:x>871</pd:x>
        <pd:y>34</pd:y>
        <config>
            <variableName>Var</variableName>
        </config>
        <pd:inputBindings>
            <root>
                <message>
                    <xsl:value-of select="$_globalVariables/pfx3:GlobalVariables/VarAplication/MessageType/Error"/>
                </message>
                <error>
                    <xsl:value-of select="&quot;Create New Service-Invalid Inputs&quot;"/>
                </error>
            </root>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Catch">
        <pd:type>com.tibco.pe.core.CatchActivity</pd:type>
        <pd:resourceType>ae.activities.catch</pd:resourceType>
        <pd:x>1004</pd:x>
        <pd:y>34</pd:y>
        <pd:handler>true</pd:handler>
        <config>
            <catchAll>true</catchAll>
        </config>
        <pd:inputBindings/>
    </pd:activity>
    <pd:transition>
        <pd:from>Start</pd:from>
        <pd:to>Get Worker by ID</pd:to>
        <pd:xpathDescription>validation</pd:xpathDescription>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>xpath</pd:conditionType>
        <pd:xpath>$Start/pfx8:New_Service_In_msg/header/pfx7:Standard_In_MSG/@id &gt; 0 
and 
$Start/pfx8:New_Service_In_msg/header/pfx7:Standard_In_MSG/@id &lt; 10000000000
and
$Start/pfx8:New_Service_In_msg/header/pfx7:Standard_In_MSG/@role = $_globalVariables/pfx3:GlobalVariables/VarAplication/RoleType/Admin</pd:xpath>
    </pd:transition>
    <pd:transition>
        <pd:from>Get Worker by ID</pd:from>
        <pd:to>New Service</pd:to>
        <pd:xpathDescription>valid_worker</pd:xpathDescription>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>xpath</pd:conditionType>
        <pd:xpath>$Get-Worker-by-ID/root/message = $_globalVariables/pfx3:GlobalVariables/VarAplication/MessageType/Exist
and
$Start/pfx8:New_Service_In_msg/header/pfx7:Standard_In_MSG/@role = $Get-Worker-by-ID/root/WorkerInfo/Role</pd:xpath>
    </pd:transition>
    <pd:transition>
        <pd:from>New Service</pd:from>
        <pd:to>Assign 1</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Assign 1</pd:from>
        <pd:to>End</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Assign 2</pd:from>
        <pd:to>End</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Catch</pd:from>
        <pd:to>Assign 2</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
