                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="message" type="xsd:string"/>
                        <xsd:element name="error" type="xsd:string" minOccurs="0"/>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
        </Var>
    </pd:processVariables>
    <pd:targetNamespace>http://xmlns.example.com/1563185982404</pd:targetNamespace>
    <pd:activity name="Get Worker by ID">
        <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
        <pd:resourceType>ae.process.subprocess</pd:resourceType>
        <pd:x>175</pd:x>
        <pd:y>34</pd:y>
        <config>
            <processName>/Processes/Read DB/Workers Table/Get Worker by ID.process</processName>
        </config>
        <pd:inputBindings>
            <root>
                <WorkerNumber>
                    <xsl:value-of select="$Start/pfx8:Change_Associated_Service_In_msg/header/pfx6:Standard_In_MSG/@id"/>
                </WorkerNumber>
            </root>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Get Service by ID">
        <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
        <pd:resourceType>ae.process.subprocess</pd:resourceType>
        <pd:x>328</pd:x>
        <pd:y>34</pd:y>
        <config>
            <processName>/Processes/Read DB/Services Table/Get Service by ID.process</processName>
        </config>
        <pd:inputBindings>
            <root>
                <ServiceID>
                    <xsl:value-of select="$Start/pfx8:Change_Associated_Service_In_msg/body/pfx7:Modify_Associated_Service_W/@serviceID"/>
                </ServiceID>
            </root>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Associated Service Counter">
        <pd:type>com.tibco.plugin.jdbc.JDBCQueryActivity</pd:type>
        <pd:resourceType>ae.activities.JDBCQueryActivity</pd:resourceType>
        <pd:x>477</pd:x>
        <pd:y>42</pd:y>
        <config>
            <timeout>10</timeout>
            <commit>false</commit>
            <maxRows>100</maxRows>
            <emptyStrAsNil>false</emptyStrAsNil>
            <statement>SELECT COUNT(*) 
FROM associatedservices
WHERE associatedserviceid = ?</statement>
            <Prepared_Param_DataType>
                <parameter>
                    <parameterName>AssociatedServiceID</parameterName>
                    <dataType>INTEGER</dataType>
                </parameter>
            </Prepared_Param_DataType>
            <jdbcSharedConfig>/Resources/JDBC/JDBC Connection.sharedjdbc</jdbcSharedConfig>
            <oraObjects/>
            <oraTables/>
            <QueryOutputCachedSchemaColumns>COUNT</QueryOutputCachedSchemaColumns>
            <QueryOutputCachedSchemaDataTypes>-5</QueryOutputCachedSchemaDataTypes>
            <QueryOutputCachedSchemaDataTypesName>BIGINT</QueryOutputCachedSchemaDataTypesName>
            <QueryOutputCachedSchemaStatus>RequiredElement</QueryOutputCachedSchemaStatus>
        </config>
        <pd:inputBindings>
            <jdbcQueryActivityInput>
                <AssociatedServiceID>
                    <xsl:choose>
                        <xsl:when test="exists($Start/pfx8:Change_Associated_Service_In_msg/body/pfx7:Modify_Associated_Service_W/@associatedServiceID)">
                            <xsl:value-of select="$Start/pfx8:Change_Associated_Service_In_msg/body/pfx7:Modify_Associated_Service_W/@associatedServiceID"/>
                        </xsl:when>
                        <xsl:otherwise>
                            <xsl:attribute name="xsi:nil">true</xsl:attribute>
                        </xsl:otherwise>
                    </xsl:choose>
                </AssociatedServiceID>
            </jdbcQueryActivityInput>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Assign 1">
        <pd:type>com.tibco.pe.core.AssignActivity</pd:type>
        <pd:resourceType>ae.activities.assignActivity</pd:resourceType>
        <pd:x>903</pd:x>
        <pd:y>34</pd:y>
        <config>
            <variableName>Var</variableName>
        </config>
        <pd:inputBindings>
            <root>
                <message>
                    <xsl:value-of select="$Modify-Associated-Service/root/message"/>
                </message>
                <xsl:if test="$Modify-Associated-Service/root/error">
                    <error>
                        <xsl:value-of select="$Modify-Associated-Service/root/error"/>
                    </error>
                </xsl:if>
            </root>
        </pd:inputBindings>
    </pd:activity>
