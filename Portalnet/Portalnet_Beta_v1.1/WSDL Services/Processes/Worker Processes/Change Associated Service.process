    <pd:transition>
        <pd:from>Start</pd:from>
        <pd:to>Get Worker by ID</pd:to>
        <pd:xpathDescription>validation</pd:xpathDescription>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>xpath</pd:conditionType>
        <pd:xpath>( $Start/pfx8:Change_Associated_Service_In_msg/header/pfx6:Standard_In_MSG/@id &gt; 0 and $Start/pfx8:Change_Associated_Service_In_msg/header/pfx6:Standard_In_MSG/@id &lt; 10000000000 )
and
( $Start/pfx8:Change_Associated_Service_In_msg/body/pfx7:Modify_Associated_Service_W/@serviceID &gt; 0 and $Start/pfx8:Change_Associated_Service_In_msg/body/pfx7:Modify_Associated_Service_W/@serviceID &lt; 100000 )
and
( $Start/pfx8:Change_Associated_Service_In_msg/body/pfx7:Modify_Associated_Service_W/@associatedServiceID &gt; 0 and $Start/pfx8:Change_Associated_Service_In_msg/body/pfx7:Modify_Associated_Service_W/@associatedServiceID &lt; 100000000000 )
and
(
	$Start/pfx8:Change_Associated_Service_In_msg/header/pfx6:Standard_In_MSG/@role = $_globalVariables/pfx4:GlobalVariables/VarAplication/RoleType/Admin
	or
	$Start/pfx8:Change_Associated_Service_In_msg/header/pfx6:Standard_In_MSG/@role = $_globalVariables/pfx4:GlobalVariables/VarAplication/RoleType/Operator
)</pd:xpath>
    </pd:transition>
    <pd:transition>
        <pd:from>Get Worker by ID</pd:from>
        <pd:to>Get Service by ID</pd:to>
        <pd:xpathDescription>valid_worker</pd:xpathDescription>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>xpath</pd:conditionType>
        <pd:xpath>$Get-Worker-by-ID/root/message = $_globalVariables/pfx4:GlobalVariables/VarAplication/MessageType/Exist
and
$Start/pfx8:Change_Associated_Service_In_msg/header/pfx6:Standard_In_MSG/@role = $Get-Worker-by-ID/root/WorkerInfo/Role</pd:xpath>
    </pd:transition>
    <pd:transition>
        <pd:from>Get Service by ID</pd:from>
        <pd:to>Associated Service Counter</pd:to>
        <pd:xpathDescription>valid_service</pd:xpathDescription>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>xpath</pd:conditionType>
        <pd:xpath>$Get-Service-by-ID/root/message = $_globalVariables/pfx4:GlobalVariables/VarAplication/MessageType/Exist</pd:xpath>
    </pd:transition>
    <pd:transition>
        <pd:from>Associated Service Counter</pd:from>
        <pd:to>Modify Associated Service</pd:to>
        <pd:xpathDescription>exist</pd:xpathDescription>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>xpath</pd:conditionType>
        <pd:xpath>$Associated-Service-Counter/resultSet/Record/COUNT = 1</pd:xpath>
    </pd:transition>
