        <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
        <pd:resourceType>ae.process.subprocess</pd:resourceType>
        <pd:x>767</pd:x>
        <pd:y>42</pd:y>
        <config>
            <processName>/Processes/Modify DB/Associated Services Table/Modify Associated Service.process</processName>
        </config>
        <pd:inputBindings>
            <root>
                <AssociatedServiceID>
                    <xsl:value-of select="$Start/pfx8:Change_Associated_Service_In_msg/body/pfx7:Modify_Associated_Service_W/@associatedServiceID"/>
                </AssociatedServiceID>
                <ServiceID>
                    <xsl:value-of select="$Start/pfx8:Change_Associated_Service_In_msg/body/pfx7:Modify_Associated_Service_W/@serviceID"/>
                </ServiceID>
                <WorkerNumber>
                    <xsl:value-of select="$Start/pfx8:Change_Associated_Service_In_msg/header/pfx6:Standard_In_MSG/@id"/>
                </WorkerNumber>
                <ModificationDate>
                    <xsl:value-of select="$Start/pfx8:Change_Associated_Service_In_msg/body/pfx7:Modify_Associated_Service_W/@modificationDate"/>
                </ModificationDate>
            </root>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Assign 2">
        <pd:type>com.tibco.pe.core.AssignActivity</pd:type>
        <pd:resourceType>ae.activities.assignActivity</pd:resourceType>
        <pd:x>1168</pd:x>
        <pd:y>34</pd:y>
        <config>
            <variableName>Var</variableName>
        </config>
        <pd:inputBindings>
            <root>
                <message>
                    <xsl:value-of select="$_globalVariables/pfx4:GlobalVariables/VarAplication/MessageType/Error"/>
                </message>
                <error>
                    <xsl:value-of select="&quot;Change Associated Service-Invalid Inputs&quot;"/>
                </error>
            </root>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Catch">
        <pd:type>com.tibco.pe.core.CatchActivity</pd:type>
        <pd:resourceType>ae.activities.catch</pd:resourceType>
        <pd:x>1297</pd:x>
        <pd:y>34</pd:y>
        <pd:handler>true</pd:handler>
        <config>
            <catchAll>true</catchAll>
        </config>
        <pd:inputBindings/>
