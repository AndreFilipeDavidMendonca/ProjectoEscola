    <wsdl:import namespace="http://xmlns.example.com/1563529189768" location="/WSDL Services/Aplication/Worker WSDL.wsdl"/>
    <pd:name>WSDL Services/Processes/Worker Processes/Change Associated Service.process</pd:name>
    <pd:startName>Start</pd:startName>
    <pd:startType wsMsgRef="pfx8:Change_Associated_Service_In_msg"/>
    <pd:startX>27</pd:startX>
    <pd:startY>34</pd:startY>
    <pd:returnBindings>
