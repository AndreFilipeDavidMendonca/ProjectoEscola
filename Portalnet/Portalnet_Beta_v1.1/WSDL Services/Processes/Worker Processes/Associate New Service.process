( $Start/pfx8:Associate_Service_In_msg/body/pfx7:New_Associated_Service_W/@serviceID &gt; 0 and $Start/pfx8:Associate_Service_In_msg/body/pfx7:New_Associated_Service_W/@serviceID &lt; 100000 )
