( $Start/pfx8:Associate_Service_In_msg/body/pfx7:New_Associated_Service_W/@clientNumber &gt; 0 and $Start/pfx8:Associate_Service_In_msg/body/pfx7:New_Associated_Service_W/@clientNumber &lt; 10000000000 )
