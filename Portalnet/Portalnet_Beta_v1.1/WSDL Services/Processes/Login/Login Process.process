                        <xsl:value-of select="$Verify-Worker-Password/root/error"/>
                    </error>
                </xsl:if>
                <xsl:if test="$Verify-Worker-Password/root/Role">
                    <role>
                        <xsl:value-of select="$Verify-Worker-Password/root/Role"/>
                    </role>
                </xsl:if>
                <xsl:if test="$Verify-Worker-Password/root/WorkerNumber">
                    <id>
                        <xsl:value-of select="$Verify-Worker-Password/root/WorkerNumber"/>
                    </id>
                </xsl:if>
            </root>
        </pd:inputBindings>
    </pd:activity>
    <pd:transition>
        <pd:from>Start</pd:from>
        <pd:to>Verify Client Password</pd:to>
        <pd:xpathDescription>not_worker</pd:xpathDescription>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>xpath</pd:conditionType>
        <pd:xpath>not( contains($Start/pfx4:Login_In_msg/login_in/pfx2:Login_In/@email, $_globalVariables/ns1:GlobalVariables/VarAplication/TestVars/WorkersDomain) )</pd:xpath>
    </pd:transition>
    <pd:transition>
        <pd:from>Verify Worker Password</pd:from>
        <pd:to>Assign 2</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Start</pd:from>
        <pd:to>Verify Worker Password</pd:to>
        <pd:xpathDescription>is_worker</pd:xpathDescription>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>xpath</pd:conditionType>
        <pd:xpath>contains($Start/pfx4:Login_In_msg/login_in/pfx2:Login_In/@email, $_globalVariables/ns1:GlobalVariables/VarAplication/TestVars/WorkersDomain)</pd:xpath>
    </pd:transition>
    <pd:transition>
        <pd:from>Verify Client Password</pd:from>
        <pd:to>Assign 1</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Assign Error</pd:from>
        <pd:to>End</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Catch</pd:from>
        <pd:to>Assign Error</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Assign 1</pd:from>
        <pd:to>End</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Assign 2</pd:from>
        <pd:to>End</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
