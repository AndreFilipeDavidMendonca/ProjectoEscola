        <pd:resourceType>ae.process.subprocess</pd:resourceType>
        <pd:x>160</pd:x>
        <pd:y>43</pd:y>
        <config>
            <processName>/Processes/Read DB/Clients Table/Verify Client Password.process</processName>
        </config>
        <pd:inputBindings>
            <root>
                <Email>
                    <xsl:value-of select="$Start/pfx4:Login_In_msg/login_in/pfx2:Login_In/@email"/>
                </Email>
                <Password>
                    <xsl:value-of select="$Start/pfx4:Login_In_msg/login_in/pfx2:Login_In/@password"/>
                </Password>
            </root>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Verify Worker Password">
        <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
        <pd:resourceType>ae.process.subprocess</pd:resourceType>
