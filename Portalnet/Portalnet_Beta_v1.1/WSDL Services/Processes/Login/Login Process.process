        <pd:y>233</pd:y>
        <config>
            <processName>/Processes/Read DB/Workers Table/Verify Worker Password.process</processName>
        </config>
        <pd:inputBindings>
            <root>
                <Email>
                    <xsl:value-of select="$Start/pfx4:Login_In_msg/login_in/pfx2:Login_In/@email"/>
                </Email>
                <Password>
                    <xsl:value-of select="$Start/pfx4:Login_In_msg/login_in/pfx2:Login_In/@password"/>
                </Password>
            </root>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Assign Error">
        <pd:type>com.tibco.pe.core.AssignActivity</pd:type>
        <pd:resourceType>ae.activities.assignActivity</pd:resourceType>
        <pd:x>429</pd:x>
        <pd:y>34</pd:y>
        <config>
            <variableName>Var</variableName>
        </config>
        <pd:inputBindings>
            <root>
                <message>
                    <xsl:value-of select="$_globalVariables/ns1:GlobalVariables/VarAplication/MessageType/Error"/>
                </message>
                <error>
                    <xsl:value-of select="&quot;Login-Invalid Input&quot;"/>
                </error>
            </root>
        </pd:inputBindings>
