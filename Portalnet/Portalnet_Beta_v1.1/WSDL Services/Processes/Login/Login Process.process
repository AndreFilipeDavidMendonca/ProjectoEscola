    <pd:activity name="Catch">
        <pd:type>com.tibco.pe.core.CatchActivity</pd:type>
        <pd:resourceType>ae.activities.catch</pd:resourceType>
        <pd:x>583</pd:x>
        <pd:y>34</pd:y>
        <pd:handler>true</pd:handler>
        <config>
            <catchAll>true</catchAll>
        </config>
        <pd:inputBindings/>
    </pd:activity>
    <pd:activity name="Assign 1">
        <pd:type>com.tibco.pe.core.AssignActivity</pd:type>
        <pd:resourceType>ae.activities.assignActivity</pd:resourceType>
        <pd:x>301</pd:x>
        <pd:y>34</pd:y>
        <config>
            <variableName>Var</variableName>
        </config>
        <pd:inputBindings>
            <root>
                <message>
                    <xsl:value-of select="$Verify-Client-Password/root/message"/>
                </message>
                <xsl:if test="$Verify-Client-Password/root/error">
                    <error>
                        <xsl:value-of select="$Verify-Client-Password/root/error"/>
                    </error>
                </xsl:if>
                <role>
                    <xsl:value-of select="$_globalVariables/ns1:GlobalVariables/VarAplication/RoleType/Client"/>
                </role>
                <xsl:if test="$Verify-Client-Password/root/ClientNumber">
                    <id>
                        <xsl:value-of select="$Verify-Client-Password/root/ClientNumber"/>
                    </id>
                </xsl:if>
            </root>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Assign 2">
        <pd:type>com.tibco.pe.core.AssignActivity</pd:type>
        <pd:resourceType>ae.activities.assignActivity</pd:resourceType>
        <pd:x>299</pd:x>
        <pd:y>228</pd:y>
        <config>
            <variableName>Var</variableName>
        </config>
        <pd:inputBindings>
            <root>
                <message>
                    <xsl:value-of select="$Verify-Worker-Password/root/message"/>
                </message>
                <xsl:if test="$Verify-Worker-Password/root/error">
