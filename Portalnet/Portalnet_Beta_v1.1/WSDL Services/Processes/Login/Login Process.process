<?xml version="1.0" encoding="UTF-8"?>
<pd:ProcessDefinition xmlns:xsl="http://www.w3.org/1999/XSL/Transform" xmlns:pfx="http://xmlns.example.com/1562858436795" xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/" xmlns:pd="http://xmlns.tibco.com/bw/process/2003" xmlns:pfx2="http://www.tibco.com/schemas/Portalnet_Beta_v1.0/Resources/Schemas/WSDL.xsd" xmlns:ns="http://www.tibco.com/schemas/Portalnet_Beta_v1.0/Resources/Schemas/Schema1.xsd" xmlns:pfx3="http://xmlns.example.com/1563380718646" xmlns:pfx4="http://xmlns.example.com/1563529122978" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:ns1="http://www.tibco.com/pe/DeployedVarsType">
    <wsdl:import namespace="http://xmlns.example.com/1562858436795"/>
    <wsdl:import namespace="http://xmlns.example.com/1563380718646"/>
    <wsdl:import namespace="http://xmlns.example.com/1563529122978" location="/WSDL Services/Aplication/Login WSDL.wsdl"/>
    <pd:name>WSDL Services/Processes/Login/Login Process.process</pd:name>
    <pd:startName>Start</pd:startName>
    <pd:startType wsMsgRef="pfx4:Login_In_msg"/>
    <pd:startX>27</pd:startX>
    <pd:startY>140</pd:startY>
    <pd:returnBindings>
        <pfx4:Login_Out_msg>
            <xsl:choose>
                <xsl:when test="$Var/root/message = $_globalVariables/ns1:GlobalVariables/VarAplication/MessageType/Success">
                    <login_out>
                        <pfx2:Login_Out>
                            <xsl:if test="$Var/root/role">
                                <xsl:attribute name="role">
                                    <xsl:value-of select="$Var/root/role"/>
                                </xsl:attribute>
                            </xsl:if>
                            <xsl:if test="$Var/root/id">
                                <xsl:attribute name="id">
                                    <xsl:value-of select="$Var/root/id"/>
                                </xsl:attribute>
                            </xsl:if>
                            <pfx2:Standard_Out_MSG>
                                <xsl:attribute name="message">
                                    <xsl:value-of select="$Var/root/message"/>
                                </xsl:attribute>
                                <xsl:if test="$Var/root/error">
                                    <xsl:attribute name="error">
                                        <xsl:value-of select="$Var/root/error"/>
                                    </xsl:attribute>
                                </xsl:if>
                            </pfx2:Standard_Out_MSG>
                        </pfx2:Login_Out>
                    </login_out>
                </xsl:when>
                <xsl:otherwise>
                    <login_out>
                        <pfx2:Login_Out>
                            <pfx2:Standard_Out_MSG>
                                <xsl:attribute name="message">
                                    <xsl:value-of select="$_globalVariables/ns1:GlobalVariables/VarAplication/MessageType/Error"/>
                                </xsl:attribute>
                                <xsl:attribute name="error">
                                    <xsl:value-of select="&quot;Login Process-Access Denied&quot;"/>
                                </xsl:attribute>
                            </pfx2:Standard_Out_MSG>
                        </pfx2:Login_Out>
                    </login_out>
                </xsl:otherwise>
            </xsl:choose>
        </pfx4:Login_Out_msg>
    </pd:returnBindings>
    <pd:endName>End</pd:endName>
    <pd:endType wsMsgRef="pfx4:Login_Out_msg"/>
    <pd:endX>431</pd:endX>
    <pd:endY>139</pd:endY>
    <pd:errorSchemas/>
    <pd:processVariables>
        <Var>
            <xsd:element name="root">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="message" type="xsd:string"/>
                        <xsd:element name="error" type="xsd:string" minOccurs="0"/>
                        <xsd:element name="role" type="xsd:string" minOccurs="0"/>
                        <xsd:element name="id" type="xsd:string" minOccurs="0"/>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
        </Var>
    </pd:processVariables>
    <pd:targetNamespace>http://xmlns.example.com/1562858876413</pd:targetNamespace>
    <pd:activity name="Verify Client Password">
